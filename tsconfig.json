{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": false,
		"strict": true,
		"outDir": "dist",
		"baseUrl": "src"
	},
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules",
		"dist",
		".vscode"
	]
}